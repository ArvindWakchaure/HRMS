let CheckinPage = class CheckinPage {
    constructor(db, geolocation, toastController) {
        this.db = db;
        this.geolocation = geolocation;
        this.toastController = toastController;
    }
    ngOnInit() {
        this.para = JSON.parse(localStorage.getItem('para'));
        console.log("Device IP: ", localStorage.getItem('IP'));
        setTimeout(() => {
            this.checkIn();
            this.getLocation();
        }, 1000);
    }
    getLocation() {
        this.geolocation.getCurrentPosition().then((resp) => {
            this.latitude = resp.coords.latitude;
            this.longitude = resp.coords.longitude;
            console.log("lat: " + this.latitude + "===" + "lang: " + this.longitude);
        }).catch((error) => {
            console.log('Error getting location', error);
        });
    }
    checkIn() {
        this.db.checkIn(this.para).subscribe((res) => (0,tslib__WEBPACK_IMPORTED_MODULE_4__.__awaiter)(this, void 0, void 0, function* () {
            console.log("Check In Response: ", res);
            if (res.isSuccess == true) {
                const toast = yield this.toastController.create({
                    message: ' You are authorized to make attendance with mobile',
                    duration: 3000
                });
                toast.present();
            }
            else if (res == "You are not allowed to make attendance with mobile") {
                const toast = yield this.toastController.create({
                    message: 'You are not allowed to make attendance with mobile',
                    duration: 3000
                });
                toast.present();
            }
        }), (error) => (0,tslib__WEBPACK_IMPORTED_MODULE_4__.__awaiter)(this, void 0, void 0, function* () {
            console.log(error);
            const toast = yield this.toastController.create({
                message: 'Invalid employee code..',
                duration: 2000
            });
            toast.present();
        }));
    }
};
